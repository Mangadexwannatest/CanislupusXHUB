   
   local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
   local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
   local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
   
   local Window = Fluent:CreateWindow({
       Title = "Fluent " .. Fluent.Version,
       SubTitle = "by dawid",
       TabWidth = 160,
       Size = UDim2.fromOffset(580, 460),
       Acrylic = true,
       Theme = "Dark",
       MinimizeKey = Enum.KeyCode.LeftControl
   })
   local Tabs = {
       Main = Window:AddTab({ Title = "Main", Icon = "" }),
   }
   
   local Options = Fluent.Options
   
   do
   local Weapon = {}
   for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    if v:isA("Tool") then
        table.insert(Weapon,v.Name)
    end
end
   
       local Dropdown = Tabs.Main:AddDropdown("Dropdown", {
           Title = "Dropdown",
           Values = Weapon,
           Multi = false,
           Default = nil,
       })
   
   
       Tabs.Main:AddButton({
           Title = "Button",
           Description = "Very important button",
           Callback = function()
            Weapon = {}
            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
             if v:isA("Tool") then
                table.insert(Weapon,v.Name)
                Dropdown:SetValue(nil)
                Dropdown:SetValues(Weapon)
             end
         end
    end})
    local function getClosestMob()
        local dist, thing = math.huge
            for i, v in next, game.Workspace.Monster.Mon:GetChildren() do
                if v.ClassName == "Model" then
                    local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
                    if mag < dist then
                        dist = mag 
                        thing = v
                    end
                end
            end
        return thing
    end
    
    
       Tabs.Main:AddButton({
        Title = "SkillZ",
        Description = "Very important button",
        Callback = function()
            game:GetService("ReplicatedStorage").Chest.Remotes.Functions.SkillAction:InvokeServer("DF_"..Options.Dropdown.Value.."_Z",{["Type"]= "Down",["MouseHit"]=getClosestMob().HumanoidRootPart.CFrame})
            game:GetService("ReplicatedStorage").Chest.Remotes.Functions.SkillAction:InvokeServer("DF_"..Options.Dropdown.Value.."_Z",{["Type"]= "Up",["MouseHit"]=getClosestMob().HumanoidRootPart.CFrame})
        end})
   
   end --End OF Do
