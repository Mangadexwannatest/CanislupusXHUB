getgenv().CanEquip = true
function EquipCard()
    while true do wait() pcall(function ()
        if getgenv().CanEquip then
        local J = game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory
        local K = J.Frame.CardInventoryFrame.CardInventoryScrollingFrame
        local Q = J.Frame.InvOverlayOuterFrame.CardInventoryOverlay.Rarity.Text
        local C = J.Frame.InvOverlayOuterFrame.CardInventoryOverlay.OverlayUnequipButton.Label.Text
        for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.CardInventoryFrame.CardInventoryScrollingFrame:GetChildren()) do
            if not v.Name:lower():match("cardinventory") and v.ClassName == "ImageButton" then
            firesignal(v.MouseButton1Down)
                    if v.LevelReq.Text > "Lv. 141" and not v.LevelReq.Text > "Lv. 151" then
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 131" and not v.LevelReq.Text > "Lv. 141" then
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 121" and not v.LevelReq.Text > "Lv. 131" then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 113" and not v.LevelReq.Text > "Lv. 121" then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 98" and not v.LevelReq.Text > "Lv. 113" then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 83" and not v.LevelReq.Text > "Lv. 98" then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 68" and not v.LevelReq.Text > "Lv. 83"  then   
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 56" and not v.LevelReq.Text > "Lv. 68" then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 41" and not v.LevelReq.Text > "Lv. 56"  then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 26" and not v.LevelReq.Text > "Lv. 41" then    
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text > "Lv. 11" and not v.LevelReq.Text > "Lv. 26" then 
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                    elseif v.LevelReq.Text <= "Lv. 11" and not v.LevelReq.Text > "Lv. 11" then 
                        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)

                end
            end
            warn("EquipCard")
            break
            warn("TEST")
        end
        end
        end)
        end    
end

EquipCard()
