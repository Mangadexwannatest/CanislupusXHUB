local DetechCard = {}
function LevelActionCard()
    if #DetechCard ~= 0 then
        table.clear(DetechCard)
    end
    for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.CardInventoryFrame.CardInventoryScrollingFrame:GetDescendants()) do
        if v.Name == "LevelReq" and string.match(v.Text,"151") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)

        elseif v.Name == "LevelReq" and string.match(v.Text,"141") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"131") and #DetechCard < 3  then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
            
        elseif v.Name == "LevelReq" and string.match(v.Text,"121") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"113") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"106") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"98") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"91") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"83") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"76") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"68") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"61") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"56") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"51") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"46") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"41") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"36") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"31") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"26") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
