    if not v:IsA("UIGridLayout") then
        if tonumber(v.LevelReq.Text) then
            if not OtherItem[v.Name] then
                OtherItem[v.Name] = {}
                table.remove(Other_Num,table.find(Other_Num,'None'))
            end
            table.insert(OtherItem[v.Name] , tonumber(v.LevelReq.Text))
        end
    end
end)

