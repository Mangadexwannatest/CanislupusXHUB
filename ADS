            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"16") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"11") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"6") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
    
        elseif v.Name == "LevelReq" and string.match(v.Text,"1") and #DetechCard < 3 then
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            table.insert(DetechCard,v.Name)
        end
    end
     
end
