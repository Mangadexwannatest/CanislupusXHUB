                  },
                  ["thumbnail"] = {
                    ["url"] = "https://tr.rbxcdn.com/1fb0c8f3fe70c5ec485cfab729d4ac44/150/150/Image/Webp",
                  },
                  ["fields"] = {
                    {
                        ["name"] = "**Item Rewards**",
                        ["value"] = "\n`[+]` "..table.concat(Item_Rewards,"\n`[+]` "),
