local RarityLevel = {}
function Rarity()
    return game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.InvOverlayOuterFrame.CardInventoryOverlay.Rarity.Text
end
function Rarity_Level_Action()
    if #RarityLevel ~= 0 then
        table.clear(RarityLevel)
    end
    for i,v in pairs(game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.Inventory.Frame.CardInventoryFrame.CardInventoryScrollingFrame:GetChildren()) do
        Lvl = game.Players.LocalPlayer.leaderstats.Level.Value
        if v:IsA("ImageButton") and string.match(v.LevelReq.Text,"151") and Lvl > 150 then
            task.wait()
            firesignal(v.MouseButton1Down,game.Players.LocalPlayer)
            if Rarity() == "Mythic" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Legendary" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Epic" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Rare" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Uncommon" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Common" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
        elseif v:IsA("ImageButton") and string.match(v.LevelReq.Text,"141") and #RarityLevel < 3 and Lvl > 140 and Lvl < 150 then
            task.wait()
            firesignal(v.MouseButton1Down,game.Players.LocalPlayer)
            if Rarity() == "Mythic" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Legendary" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
                table.insert(RarityLevel,v.Name)
            end
            if Rarity() == "Epic" and #RarityLevel < 3 then
                game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Name)
