        elseif v.Name == "LevelReq" and v.Text == "Lv. 21" and #DetechCard < 3 then
            table.insert(DetechCard,v.Name)
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            
    
        elseif v.Name == "LevelReq" and v.Text == "Lv. 16" and #DetechCard < 3 then
            table.insert(DetechCard,v.Name)
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            
    
    
        elseif v.Name == "LevelReq" and v.Text == "Lv. 11" and #DetechCard < 3 then
            table.insert(DetechCard,v.Name)
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            
    
        elseif v.Name == "LevelReq" and v.Text == "Lv. 6" and #DetechCard < 3 then
            table.insert(DetechCard,v.Name)
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            
    
        elseif v.Name == "LevelReq" and v.Text == "Lv. 1" and #DetechCard < 3 then
            table.insert(DetechCard,v.Name)
            game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("EquipCard",v.Parent.Name)
            
        end
    end
     
