    NO_CLIP_PART.Anchored = true
    NO_CLIP_PART.Transparency = 1
    NO_CLIP_PART.Size = Vector3.new(2,1,2)
elseif game.Players.LocalPlayer.Character:FindFirstChild("CrazyDay_NO") and Options.AutoFarmAndStart.Value then
    repeat
        wait()
        game.Players.LocalPlayer.Character:FindFirstChild("CrazyDay_NO").CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-3.5,0)
     until not game.Players.LocalPlayer.Character:FindFirstChild("CrazyDay_NO")  or not Options.AutoFarmAndStart.Value
    end
end)
end
end))


