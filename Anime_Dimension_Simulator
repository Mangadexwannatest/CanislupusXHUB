    })

    Tabs.Skill:AddDropdown("Fourth_Skill", {
        Title = "Fourth First",
        Values = {"1","2","3","4","5"},
        Multi = false,
        Default = getgenv().Fourth_Skill or 4,
    })

    Tabs.Skill:AddDropdown("Fifth_Skill", {
        Title = "Fifth First",
        Values = {"1","2","3","4","5"},
        Multi = false,
        Default = getgenv().Fifth_Skill or 5,
    })

    Tabs.Skill:AddToggle("Auto_Skill_Assist", {Title = "Auto Skill // Assist", Default = getgenv().Auto_Skill_Assist })

    


    Tabs.Webhook:AddInput("Webhook", {
