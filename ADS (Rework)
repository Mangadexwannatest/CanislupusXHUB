		elseif #Options["Current Rarity"].Value == 5 then
			Options["Current Rarity"]:SetValue({
				[Options["Current Rarity"].Value[1]] = true,
				[Options["Current Rarity"].Value[2]] = true,
				[Options["Current Rarity"].Value[3]] = true,
